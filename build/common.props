<Project>
  <Import Project="version.props" />
  <PropertyGroup>
    <Product Condition=" '$(Product)' == '' ">Episerver CMS</Product>
    <Description Condition=" '$(Description)' == '' ">Episerver Web Content Management System</Description>
    <Company>Episerver AB</Company>
    <Authors>Episerver AB</Authors>
    <Copyright>Â© 2003-2017 by Episerver AB. All rights reserved</Copyright>
    <PackageProjectUrl>http://www.episerver.com/web-content-management/</PackageProjectUrl>
    <PackageLicenseUrl>http://www.episerver.com/eula/</PackageLicenseUrl>
    <PackageTags>EPiServer Search</PackageTags>
    <DelaySign>true</DelaySign>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)EPiServerProduct_PublicKey.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <PublicSign Condition=" '$(OS)' != 'Windows_NT' ">true</PublicSign>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>
</Project>