{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "EPiServer": {
    "episerver.search": {
      "active": true,
      "namedIndexingServices": {
        "defaultService": "serviceName",
        "services": [
          {
            "name": "serviceName",
            "baseUri": "https://<yoursearchsite>/api/indexing",
            "accessKey": "<accesskey>"
          }
        ]
      },
      "searchResultFilter": {
        "defaultInclude": "true",
        "providers": [

        ]
      }
    }
  }
}
