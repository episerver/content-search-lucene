{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "EPiServer": {
    "episerver.search.indexingservice": {
      "Clients": [
        {
          "Name": "local",
          "Description": "local",
          "AllowLocal": true,
          "ReadOnly": false
        }
      ],
      "NamedIndexes": {
        "DefaultIndex": "default",
        "Indexes": [
          {
            "Name": "default",
            "DirectoryPath": "[appDataPath]\\Index",
            "ReadOnly": false
          }
        ]
      }
    }
  }
}
